@article{ ISI:000454382500012,
Author = {Papic, Milorad and Bundalo, Zlatko and Bundalo, Dusanka and Stojanovic,
   Radovan and Kovacevic, Zivorad and Pasalic, Drazen and Cvijic, Branimir},
Title = {{Microcomputer based embedded SCADA and RFID systems implemented on LINUX
