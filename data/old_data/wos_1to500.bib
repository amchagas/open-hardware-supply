Booktitle = {{2017 14TH INTERNATIONAL CONFERENCE ON UBIQUITOUS ROBOTS AND AMBIENT
   INTELLIGENCE (URAI)}},
Series = {{International Conference on Ubiquitous Robots and Ambient Intelligence}},
Year = {{2017}},
