@article{ ISI:000483155000001,
Author = {Del Barrio, Alberto A. and Manzano, Jose P. and Maroto, Victor M. and
   Villarin, Alvaro and Pagan, Josue and Zapater, Marina and Ayala, Jose
   and Hermida, Roman},
Title = {{HackRF plus GNU Radio: A software-defined radio to teach communication
   theory}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTRICAL ENGINEERING EDUCATION}},
Abstract = {{In this paper, an alternative to the traditional methodology related to
   signal processing-like subjects is proposed. These are subjects that
   require a deep mathematical and theoretical basis, but the practical
   goal is not often emphasized, which drives students to lose interest in
   the subject. Thus, a software-defined radio environment is proposed to
   provide a more practical view of the subject. This solution consists of
   an open hardware-software platform able to capture and process a wide
   range of frequencies. HackRF is the hardware component, while GNU Radio
   will provide the graphical support to this device. The tests performed
   with a set of 36 students have revealed that they are more satisfied
   with this framework than just employing a traditional equation-based
   environment as Matlab. Furthermore, their scores in the exams also
   support the suitability of the proposed platform.}},
