@article{ ISI:000447833100019,
Author = {Hafid, Abdelakram and Benouar, Sara and Kedir-talha, Malika and Abtahi,
   Farhad and Attari, Mokhtar and Seoane, Fernando},
Title = {{Full Impedance Cardiography Measurement Device Using Raspberry PI3 and
   System-on-Chip Biomedical Instrumentation Solutions}},
Journal = {{IEEE JOURNAL OF BIOMEDICAL AND HEALTH INFORMATICS}},
Year = {{2018}},
Volume = {{22}},
Number = {{6, SI}},
Pages = {{1883-1894}},
Month = {{NOV}},
Abstract = {{Impedance cardiography (ICG) is a noninvasive method for monitoring
   cardiac dynamics using electrical bioimpedance (EBI) measurements. Since
   its appearance more than 40 years ago, ICG has been used for assessing
   hemodynamic parameters. This paper presents a measurement system based
   on two System on Chip (SoC) solutions and Raspberry PI, implementing
   both a full three-lead ECG recorder and an impedance cardiographer, for
   educational and research development purposes. Raspberry PI is a
   platform supporting Do-I t-Yourself project and education applications
   across the world. The development is part of Biosignal PI, an open
   hardware platform focusing in quick prototyping of physiological
   measurement instrumentation. The SoC used for sensing cardiac
   biopotential is the ADAS1000, and for the EBI measurement is the AD5933.
   The recordings were wirelessly transmitted through Bluetooth to a PC,
   where the waveforms were displayed, and hemodynamic parameters such as
   heart rate, stroke volume, ejection time and cardiac output were
   extracted from the ICG and ECG recordings. These results show how
   Raspberry PI can be used for quick prototyping using relatively widely
