@inproceedings{ ISI:000358567800225,
Author = {Sapena-Bano, A. and Perez-Cruz, J. and Pineda-Sanchez, M. and
   Puche-Panadero, R. and Roger-Folch, J. and Riera-Guasp, M. and
   Martinez-Roman, J.},
Book-Group-Author = {{IEEE}},
Title = {{Condition monitoring of electrical machines using low computing power
   devices}},
