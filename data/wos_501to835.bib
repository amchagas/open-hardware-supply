Title = {{Low Cost Cloud Based Intelligent Farm Automation System Using Bluetooth
   Low Energy}},
Booktitle = {{PROCEEDINGS OF THE 2014 IEEE REGION 10 HUMANITARIAN TECHNOLOGY
   CONFERENCE (RIO HTC)}},
Series = {{IEEE Region 10 Humanitarian Technology Conference}},
Year = {{2014}},
Pages = {{127-132}},
