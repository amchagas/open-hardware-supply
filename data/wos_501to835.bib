@inproceedings{ ISI:000380496300014,
Author = {De Maria, Carmelo and Ferrari, Laura and Montemurro, Francesca and
   Vozzi, Federico and Guerrazzi, Ilenia and Boland, Thomas and Vozzi,
   Giovanni},
Editor = {{Fernandes, PR and Bartolo, P}},
Title = {{Design and Validation of an Open-Hardware Print-Head for Bioprinting
   Application}},
Booktitle = {{4TH INTERNATIONAL CONFERENCE ON TISSUE ENGINEERING, ICTE2015, AN ECCOMAS
   THEMATIC CONFERENCE}},
Series = {{Procedia Engineering}},
Year = {{2015}},
Volume = {{110}},
