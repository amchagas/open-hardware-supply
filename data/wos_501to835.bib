@inproceedings{ ISI:000380413200041,
Author = {Muhammad, Cameron and Samanta, Biswanath},
Book-Group-Author = {{ASME}},
Title = {{CONTROL OF AUTONOMOUS ROBOTS USING PRINCIPLES OF NEUROMODULATION IN ROS
   ENVIRONMENT}},
