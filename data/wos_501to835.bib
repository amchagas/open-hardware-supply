Author = {Czerwinski, Dariusz and Przylucki, Slawomir},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{OPEN-SOURCE MICROCONTROLLER DEVELOPMENT BOARD IN WIRELESS SENSOR
