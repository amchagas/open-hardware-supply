@inproceedings{ ISI:000377956900104,
Author = {Cucci, Davide A. and Migliavacca, Martino and Bonarini, Andrea and
   Matteucci, Matteo},
Editor = {{Menegatti, E and Michael, N and Berns, K and Yamaguchi, H}},
Title = {{Development of Mobile Robots Using Off-the-Shelf Open-Source Hardware
   and Software Components for Motion and Pose Tracking}},
Booktitle = {{INTELLIGENT AUTONOMOUS SYSTEMS 13}},
Series = {{Advances in Intelligent Systems and Computing}},
Year = {{2016}},
