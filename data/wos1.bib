@article{ ISI:000528549700026,
Author = {Galvao Rosa, Alessandra de Fatima and Betini, Roberto Cesar},
Title = {{Noncontact SpO(2) Measurement Using Eulerian Video Magnification}},
Journal = {{IEEE TRANSACTIONS ON INSTRUMENTATION AND MEASUREMENT}},
Year = {{2020}},
Volume = {{69}},
Number = {{5, 1}},
Pages = {{2120-2130}},
Month = {{MAY}},
Abstract = {{Peripheral oxygen saturation (SpO(2)) monitoring is critical during
   surgery and in intensive care units (ICS). Over the last decades,
   several studies have addressed the noncontact monitoring of vital signs,
   but few reports have focused on SpO(2). This paper presents an SpO(2)
   monitoring method without physical contact with the patient using
   imaging photoplethysmography (iPPG). Eulerian video magnification (EVM)
   was employed in the proposed solution to amplify the changes in skin
   color due to the heart cycle. The developed software operates on a
   widely available, low-cost hardware platform. Experiments were performed
   with nine healthy individuals. When comparing the experimental SpO(2)
   results to those obtained with a reference pulse oximeter, the proposed
   solution showed good accuracy, with differences of less than 2\%.}},
