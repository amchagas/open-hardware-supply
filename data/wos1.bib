@inproceedings{ ISI:000469933200215,
Author = {Asman, Priscella and Jiang, Tianxiao and Ozturk, Musa and Reyna, Juan
   and Ince, Nuri F.},
Book-Group-Author = {{IEEE}},
Title = {{A Low-Cost Microcontroller Based Stimulation System to Study Sensory
   Processing}},
