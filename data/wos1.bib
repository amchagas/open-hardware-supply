@article{ ISI:000490569000004,
Author = {Hill, Andrew P. and Davies, Alasdair and Prince, Peter and Snaddon, Jake
   L. and Doncaster, C. Patrick and Rogers, Alex},
Title = {{Leveraging conservation action with open-source hardware}},
Journal = {{CONSERVATION LETTERS}},
Year = {{2019}},
Volume = {{12}},
Number = {{5}},
Month = {{SEP}},
Abstract = {{Data collection by conservation biologists is undergoing radical change,
   with researchers collaborating across disciplines to create bespoke,
   low-cost monitoring equipment from open-source hardware (OSH). Compared
   to commercial hardware, OSH dramatically reduces participation costs.
   Four barriers currently hold back its wide adoption: (1) user
   inexperience inhibits initial uptake; (2) complex and costly
   manufacturing/distribution procedures impede global dissemination; (3)
   lack of creator support results in lapsed projects; and (4) lack of user
   support degrades continued utility in the field. Here, we propose a
   framework to address these barriers, illustrating how OSH offers a route
   to rapid expansion of community-driven conservation action.}},
