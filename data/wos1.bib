Author = {Sels, S. and Bogaerts, B. and Vanlanduit, S. and Penne, R.},
Editor = {{Desmet, W and Pluymers, B and Moens, D and Rottiers, W}},
Title = {{Easy sensor localization using an RGB camera}},
Booktitle = {{PROCEEDINGS OF INTERNATIONAL CONFERENCE ON NOISE AND VIBRATION
