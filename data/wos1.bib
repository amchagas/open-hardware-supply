Author = {Murdan, Anshu Prakash and Emambocus, Muhammad Zuhayr Aliy},
Book-Group-Author = {{IEEE}},
Title = {{Indoor Positioning System Simulation for a Robot using Radio Frequency
   Identification}},
Booktitle = {{PROCEEDINGS OF THE 2018 13TH IEEE CONFERENCE ON INDUSTRIAL ELECTRONICS
   AND APPLICATIONS (ICIEA 2018)}},
Series = {{IEEE Conference on Industrial Electronics and Applications}},
Year = {{2018}},
