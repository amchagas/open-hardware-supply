@article{ ISI:000492691500005,
Author = {Saari, Mika and Sillberg, Pekka and Gronman, Jere and Kuusisto, Markku
   and Rantanen, Petri and Jaakkola, Hannu and Henno, Jaak},
Title = {{Reducing Energy Consumption with IoT Prototyping}},
