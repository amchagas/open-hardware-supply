Title = {{A Novel Bidirectional RFID Identity Authentication Protocol}},
Booktitle = {{2018 IEEE SMARTWORLD, UBIQUITOUS INTELLIGENCE \& COMPUTING, ADVANCED \&
   TRUSTED COMPUTING, SCALABLE COMPUTING \& COMMUNICATIONS, CLOUD \& BIG
   DATA COMPUTING, INTERNET OF PEOPLE AND SMART CITY INNOVATION
   (SMARTWORLD/SCALCOM/UIC/ATC/CBDCOM/IOP/SCI)}},
