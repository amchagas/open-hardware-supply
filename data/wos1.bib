@article{ ISI:000477785800010,
Author = {Bhargavapuri, Mahathi and Shastry, Animesh Kumar and Sinha, Harsh and
   Sahoo, Soumya Ranjan and Kothari, Mangal},
Title = {{Vision-based autonomous tracking and landing of a fully-actuated
   rotorcraft}},
Journal = {{CONTROL ENGINEERING PRACTICE}},
Year = {{2019}},
Volume = {{89}},
Pages = {{113-129}},
Month = {{AUG}},
Abstract = {{Autonomous landing is a challenging phase of flight for an aerial
   vehicle, especially when attempting to land on a moving target. This
   paper presents vision-based tracking and landing of a fully-actuated
   tilt-augmented quadrotor on a moving target. A fully-actuated vehicle
   allows higher freedom in terms of control design and a larger flight
   envelope since the position and attitude states are decoupled. An
   adaptive control law is designed to track a moving target with only
   relative position information from a camera. Low-cost hardware is used,
   and experiments are carried out to validate the proposed methodology for
   targets moving at realistic speeds.}},
