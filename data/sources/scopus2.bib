@CONFERENCE{Dissanayake2016,
author={Dissanayake, A. and Zarembowitch, A. and Hogie, K. and Yang, X. and Lubelczyk, J. and Safavi, H.},
title={TDRSS narrow-band simulator and test system},
journal={IEEE Aerospace Conference Proceedings},
year={2016},
volume={2016-June},
doi={10.1109/AERO.2016.7500752},
art_number={7500752},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84978517028&doi=10.1109%2fAERO.2016.7500752&partnerID=40&md5=4f166ee981288c56fed35fce9c7647e8},
affiliation={Harris Corporation, 7855 Walker Drive, Greenbelt, MD  20770, United States; MSS/Comblock, 845-N Quince Orchard Blvd., Gaithersburg, MD  20878, United States; Computer Sciences Corporation, 12401 Mount Pleasant Dr., Laurel, MD  20708, United States; Harris Corporation, NASA/GSFC, 8800 Greenbelt Road, Greenbelt, MD  20771, United States},
abstract={Introducing new communication equipment or system to the Tracking and Data Relay Satellite System (TDRSS) ground segment is a complicated process that involves a lengthy development and test cycle. Complexity arises due to the presence of the variety of signaling formats, operation and user constraints, monitor and control parameters, and the requirement to ensure the new piece of ground equipment or system meets the functional and performance requirements for all different combinations of system configuration parameters and operational scenarios. The development cycle starts with extensive software simulations to establish the performance parameters of the new equipment or system followed by a prototyping stage. The prototype system undergoes a battery of tests in a laboratory environment to verify that it is capable of meeting the performance levels derived from the software simulations. Testing must be performed using simulated signals generated either using software or hardware. The current practice is to use a chain of test equipment, each piece in the chain dedicated to a single function. This requires manual setup between different test configurations and test reporting is also handled manually. This is costly both in terms of test hardware investment and the manpower involved. The options available to reduce the cost are to use either a software or hardware simulator that can combine multiple functions in a single unit. Software simulation involves generating signal files that can be played back repeatedly. Depending on the data rate being simulated, each test file can occupy several Terabytes (TBs); in addition, it requires a playback system which tends to be relatively expensive to acquire and maintain. On the other hand, with more advanced signal processing technology the hardware simulation can be achieved with low cost Field Programmable Gate Array (FPGA). A low cost hardware simulator that can support data rates up to 25 Mbps is selected for the simulation system. The simulator supports baseband digital data formats and different modulation and coding schemes used in the TDRSS. Channel impairments applicable for communicating with orbiting platforms and signal impairments generated by the TDRSS itself are also simulated by the system; these include stressing profiles of delay, Doppler, and multipath. Hardware distortions of customer platforms are simulated by a set of software defined distortion filters designed according to the user constraints specified in the Space Network user's guide. In order to reduce the time spent during the prototype testing phase, an automated test system is being developed. Simulator architecture and the test automation approach will be presented. © 2016 IEEE.},
keywords={Chains;  Complex networks;  Computer software;  Costs;  Data communication equipment;  Equipment;  Equipment testing;  Field programmable gate arrays (FPGA);  Hardware;  Orbits;  Reconfigurable hardware;  Signal processing;  Signaling equipment;  Simulators;  Software prototyping;  Space platforms;  Tracking (position), Advanced signal processing;  Automated test systems;  Communication equipments;  Laboratory environment;  Modulation and coding schemes;  Performance parameters;  Performance requirements;  Tracking and data relay satellite system, Software testing},
publisher={IEEE Computer Society},
issn={1095323X},
isbn={9781467376761},
language={English},
abbrev_source_title={IEEE Aerosp. Conf. Proc.},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Coonley2016,
author={Coonley, K. and Manturuk, K. and Miles, J. and Lipp, G. and Lorch, C. and Woodard, C. and Brooke, M.},
title={Massive open online laboratories? Ongoing work with microelectronics experiments performed outside of the traditional laboratory},
