publisher={Institute of Electrical and Electronics Engineers Inc.},
isbn={9781538638897},
language={English},
abbrev_source_title={ISPASS - IEEE Int. Symp. Perform. Anal. Syst. Softw.},
document_type={Conference Paper},
source={Scopus},
}

