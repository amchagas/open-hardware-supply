@inproceedings{ ISI:000431859500015,
Author = {James, Laura},
Book-Group-Author = {{IEEE}},
Title = {{Opportunities and challenges of distributed manufacturing for
   humanitarian response}},
Booktitle = {{2017 IEEE GLOBAL HUMANITARIAN TECHNOLOGY CONFERENCE (GHTC)}},
