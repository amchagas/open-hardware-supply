@inproceedings{ ISI:000400784901124,
Author = {Santorelli, Adam and Porter, Emily and Dantas, Stefano and Popovic,
   Milica and Schwartz, Joshua},
Book-Group-Author = {{IEEE}},
Title = {{Low-cost hardware for a time-domain microwave system for breast health
   monitoring}},
Booktitle = {{2016 10TH EUROPEAN CONFERENCE ON ANTENNAS AND PROPAGATION (EUCAP)}},
Series = {{Proceedings of the European Conference on Antennas and Propagation}},
Year = {{2016}},
Note = {{10th European Conference on Antennas and Propagation (EuCAP), Davos,
   SWITZERLAND, APR 10-15, 2016}},
Abstract = {{In this paper, we present recent advancements in the design of a
   multilayer flexible circuit board for use in a time-domain microwave
   system for breast health monitoring. The flexible circuit contains both
   a switching network and 16 wideband antennas. This paper focuses on
   recent design improvements in the antenna array and the switching
   network. Specifically we have re-designed the switching network to
   require fewer control wires, integrated new and smaller antennas in the
   array, and modified the antenna arrangement to improve the flexibility
   of the circuit to better integrate with our clinical prototype. These
   design changes represent a step towards a more patient-friendly,
   compact, cost-effective, and wearable system design, in contrast to one
   that employs large bench-top devices.}},
Publisher = {{IEEE}},
Address = {{345 E 47TH ST, NEW YORK, NY 10017 USA}},
Type = {{Proceedings Paper}},
