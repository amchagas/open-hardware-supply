@inproceedings{ ISI:000451218701094,
Author = {Culau, Eduardo C. and Marchesan, Gregory C. and Weirich Junior, Nelson
   R. and de Oliveira, Leonardo Londero},
Book-Group-Author = {{IEEE}},
Title = {{An Efficient Single Core Flexible Processor Architecture for 4096-bit
   Montgomery Modular Multiplication and Exponentiation}},
Booktitle = {{2018 IEEE INTERNATIONAL SYMPOSIUM ON CIRCUITS AND SYSTEMS (ISCAS)}},
Series = {{IEEE International Symposium on Circuits and Systems}},
Year = {{2018}},
Note = {{IEEE International Symposium on Circuits and Systems (ISCAS), Florence,
   ITALY, MAY 27-30, 2018}},
Organization = {{IEEE; Politecnico Torino; Analog Devices; CADENCE; Texas Instruments;
   AiCTX; Springer Nat; River Publishers; Nat Electron; CAS}},
Abstract = {{This paper presents timing efficient and low cost hardware customization
   of a single core general purpose processor architecture for Montgomery
   Modular Multiplication (MMM) and exponentiation. MMM demands a lot of
   computation time and is the key for efficient cryptography applications.
   Our hybrid approach consists in elaborating an optimized C code
   (software) while customizing a Tensilica (R) Xtensa processor
   instruction set architecture in order to accelerate MMM. We substitute
   the Standard Multiplication (grade school multiplication) with a
   complexity order of O(n(2)) by a Karatsuba multiplication with a
   complexity of O(n(1:585)), so that all the critical hardware components
   are shared. Moreover, the proposed design was developed to deal with
   modulus length of 1024 up to 4096-bit. The architecture was described in
   Tensilica (R) Instruction Extension (TIE) language and synthesized in
   Cadence (R) Xtensa Xplorer. Experiments show that our design can perform
   the modular exponentiation algorithm with 167k gates in 414,708,349
   cycles considering 4096-bit modulus length in a lightweight processor
   configuration without a cache memory.}},
