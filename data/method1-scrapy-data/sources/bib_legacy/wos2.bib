@article{ ISI:000396401400016,
Author = {Boulogeorgos, Alexandros-Apostolos A. and Salameh, Haythem A. Bany and
   Karagiannidis, George K.},
Title = {{Spectrum Sensing in Full-Duplex Cognitive Radio Networks Under Hardware
