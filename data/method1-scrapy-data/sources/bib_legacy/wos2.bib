@inproceedings{ ISI:000457472400031,
Author = {Komarek, Ales and Pavlik, Jakub and Mercl, Lubos and Sobeslav, Vladimir},
Editor = {{Nguyen, NT and Papadopoulos, GA and Jedrzejowicz, P and Trawinski, B and Vossen, G}},
