@article{ ISI:000396558900006,
Author = {Ni, Xiaoyong and Zhang, Hongjian and Wang, Dianhong and Luo, Jian},
Title = {{Implementation of dynamic reconfigurable interpolator for open
   architecture CNC by using FPGA}},
Journal = {{INTERNATIONAL JOURNAL OF EMBEDDED SYSTEMS}},
Year = {{2017}},
Volume = {{9}},
Number = {{1}},
Pages = {{45-53}},
Abstract = {{Functionality and cost are two key factors for open architecture machine
   tools. Dynamic reconfiguration technology provides a method to achieve
   more functions at low cost. Hardware dynamic reconfiguration is special
   feature of FPGA devices. In this paper, a dynamic reconfigurable
   interpolator with linear, circular and NURBS interpolations is
   presented. The functions of interpolator can be reconfigured during
   machining. The requirement for logic resource on-chip reduces evidently,
