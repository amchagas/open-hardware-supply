	author = {Koch, Michael and Tumer, Irem Y.},
	editor = {{Bergendahl, MN and Grimheden, M and Leifer, L and Skogstad, P and Cantamessa, M}},
	year = {2009},
	keywords = {open design, open source hardware, collaborative engineering design, commons based peer production, concurrent engineering, open source software design},
	pages = {97+},
	annote = {17th International Conference on Engineering Design, Stanford Univ, Stanford, CA, AUG 24-27, 2009}
}

