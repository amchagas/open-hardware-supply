	booktitle = {2014 {IEEE} {FRONTIERS} {IN} {EDUCATION} {CONFERENCE} ({FIE})},
	publisher = {IEEE},
	author = {Santos, Ricardo R.},
	year = {2014},
	annote = {IEEE Frontiers in Education Conference (FIE), Madrid, SPAIN, OCT 22-25, 2014}
}

