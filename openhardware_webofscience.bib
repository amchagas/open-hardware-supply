	year = {2010},
	pages = {164--173},
	annote = {43rd Hawaii International Conference on Systems Sciences (HICSS 2010), Honolulu, HI, JAN 05-08, 2010}
}

@inproceedings{reina_anima_2010,
	address = {HEIDELBERGER PLATZ 3, D-14197 BERLIN, GERMANY},
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {{ANIMA}: {Non}-conventional {Brain}-{Computer} {Interfaces} in {Robot} {Control} through {Electroencephalography} and {Electrooculography}, {ARP} {Module}},
	volume = {6256},
	isbn = {978-3-642-15991-6},
