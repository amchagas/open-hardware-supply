	publisher = {IEEE},
	author = {Rittenbach, Thomas and Collins, Kuan and Waldman, Frank},
	year = {2009},
	pages = {431--435},
	annote = {IEEE Military Communications Conference (MILCOM 2009), Boston, MA, OCT 18-21, 2009}
}

@inproceedings{yu_fpga-based_2009,
