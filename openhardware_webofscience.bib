	author = {Mulfari, Davide and Celesti, Antonio and Fazio, Maria and Villari, Massimo and Puliafito, Antonio},
	editor = {{SikLanyi, C and Hoogerwerf, EJ and Miesenberger, K and Cudd, P}},
	year = {2015},
	keywords = {IoT, Assistive Technology, Computer accessibility, Embedded systems, Mobile solutions},
	pages = {378--385},
	annote = {13th European Conference on the Advancement of Assistive Technology (AAATE), Budapest, HUNGARY, SEP 09-12, 2015}
}

@article{hamidi_talkbox_2015,
