	year = {2015},
	pages = {481--486},
	annote = {28th International Conference on VLSI Design (VLSID) / 14th International Conference on Embedded Systems, Bangalore, INDIA, JAN 03-07, 2015}
}

@inproceedings{hussain_design_2015,
