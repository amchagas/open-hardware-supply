@inproceedings{burki_gsm_2013,
	address = {345 E 47TH ST, NEW YORK, NY 10017 USA},
	title = {{GSM} {Downlink} {Protocol} {Analysis} and {Decoding} using {Open}-{Source} {Hardware} and {Software}},
	isbn = {978-1-4799-1288-9 978-1-4799-1288-9},
	abstract = {Since its introduction in 1992, Global System for Mobile Communications (GSM) has evolved to become a worldwide standard in mobile telephony. Security in GSM, albeit being one of the most important issues, has not yet been scrutinized to a level it deserves owing mainly to the expensive hardware requirements and lack of thorough understanding of GSM protocols in general. This scenario is changing significantly with the growing rate of development of open-source tools, which can serve to harness the RF signaling and enable processing on a general purpose workstation. In this paper, these open-source tools are utilized to study the feasibility of developing a GSM Protocol Analyzer and investigating the theoretical security protocols and procedures through practical implementation of such system. Universal Software Programmable Radio Peripheral (USRP) was selected as the RF frontend for the protocol analyzer. The USRP is a general purpose, open-hardware transceiver that can be linked to a workstation via an RJ45 Gigabit Ethernet link. Various open-source software, based primarily on GNU Radio, are then utilized to synchronize, demodulate and decode the received signal.},
	language = {English},
	booktitle = {2013 {2ND} {NATIONAL} {CONFERENCE} {ON} {INFORMATION} {ASSURANCE} ({NCIA})},
	publisher = {IEEE},
	author = {Burki, Jehanzeb and Malik, Fahad and Mushtaq, Mudassar},
	year = {2013},
	keywords = {open-source, GSM, AirProbe, Ethical hacking, GNU Radio, GSM Protocol Analysis, Software Defined Radio, USRP},
	pages = {39--46},
	annote = {2nd National Conference on Information Assurance (NCIA), Natl Univ Sci \& Technol, Mil Coll Signals, Rawalpindi, PAKISTAN, DEC 11-12, 2013}
}

@inproceedings{bozicevic_http-enabled_2013,
	address = {KRUZNA 8-II, PO BOX 303, RIJEKA, HR-51001, CROATIA},
	title = {A {HTTP}-{Enabled} {Cryptographically} {Secure} {Hardware} {Random} {Number} {Generator}},
	isbn = {978-953-233-076-2},
	abstract = {A cryptographically secure hardware HTTP enabled random number generator based on a nondeterministic entropy source has been implemented using open source hardware development kit. Internally, randomness is distilled from the entropy source using Algorithm M where modified-RC4-based and BLAKE-512-as-counter-mode pseudo-random number stream generators are combined. Users access the device via HTTP on a TCP/IP network and platform specific device drivers are not required. The random number files were tested using stringent statistical tests including DIEHARD, STS 2.1.1, dieharder 3.31.1, TestU01 1.2.3 SmallCrush and ENT. Emphasis was put on low cost of hardware.},
	language = {English},
	booktitle = {2013 {36TH} {INTERNATIONAL} {CONVENTION} {ON} {INFORMATION} {AND} {COMMUNICATION} {TECHNOLOGY}, {ELECTRONICS} {AND} {MICROELECTRONICS} ({MIPRO})},
