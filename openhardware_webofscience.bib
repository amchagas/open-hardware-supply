	editor = {{Eisenbeiss, H and Kunz, M and Ingensand, H}},
	year = {2011},
	keywords = {open source, computer vision, hardware, MAVs, middleware, UAVs},
	pages = {13--18},
	annote = {International Conference on Unmanned Aerial Vehicle in Geomatics (UAV-g), Zurich, SWITZERLAND, SEP 14-16, 2011}
}

@inproceedings{meszaros_aerial_2011,
