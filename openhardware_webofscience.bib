	publisher = {ELSEVIER SCIENCE BV},
	author = {Budding, A. and Vaneker, T. H. J. and Winnubst, A. J. A.},
	editor = {{Lauwers, B and Kruth, JP}},
	year = {2013},
	keywords = {Open source hardware, Manufacturing systems, Rapid prototypes, Three Dimensional Printing},
	pages = {533--538},
	annote = {17th CIRP Conference on Electro Physical and Chemical Machining (ISEM), Leuven, BELGIUM, APR 09-12, 2013}
}

