@article{ ISI:000320720900025,
Author = {Ogiboski, L. and Pariente, C. A. B. and Magrini, L. C. and Jardini, J.
   A.},
Title = {{Real-Time System for Remote Data Acquisition in Power Station Equipments}},
Journal = {{IEEE LATIN AMERICA TRANSACTIONS}},
Year = {{2013}},
Volume = {{11}},
Number = {{2}},
Pages = {{865-869}},
Month = {{MAR}},
Abstract = {{This paper presents the specification, design and development of a
   system for remote acquisition of analogical and digital data in power
   station equipments. The real-time requirements of the acquisition system
   are of the order of milliseconds. This requirement is important for the
   generation of oscillographic data from the signals which are used to
   diagnostic the evolution of the equipment being monitored. Besides the
   real time requirements, the system is able to acquire data from other
   sources and is compatible with the most used communications protocols in
   this field of application. The contribution of this work is the
   definition of a generic system for remote data acquisition based on open
   source software and low-cost hardware. In addition, the system can be
   used for any application requiring real-time data acquisition.}},
