Author = {Afolabi, David and Man, Ka Lok and Liang, Hai-Ning and Lim, Eng Gee and
   Shen, Zhun and Lei, Chi-Un and Krilavicius, Tomas and Yang, Yue and
   Cheng, Lixin and Hahanov, Vladimir and Yemelyanov, Igor},
