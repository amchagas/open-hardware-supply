@inproceedings{ ISI:000371717207189,
Author = {Wang, Congzhi and Peng, Xi and Liang, Dong and Xiao, Yang and Qiu,
   Weibao and Qian, Ming and Zheng, Hairong},
Book-Group-Author = {{IEEE}},
Title = {{An Easily-achieved Time-domain Beamformer for Ultrafast Ultrasound
   Imaging Based on Compressive Sensing}},
Booktitle = {{2015 37TH ANNUAL INTERNATIONAL CONFERENCE OF THE IEEE ENGINEERING IN
   MEDICINE AND BIOLOGY SOCIETY (EMBC)}},
Series = {{IEEE Engineering in Medicine and Biology Society Conference Proceedings}},
