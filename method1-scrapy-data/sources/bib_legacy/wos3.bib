@inproceedings{ ISI:000384521700047,
Author = {Jose, Abin and Haak, Daniel and Jonas, Stephan M. and Brandenburg,
   Vincent and Deserno, Thomas M.},
Editor = {{Handels, H and Deserno, TM and Meinzer, HP and Tolxdorff, T}},
Title = {{Towards Standardized Wound Imaging Self-Calibrating Consumer Hardware
   Based on Lattice Detection on Color Reference Cards}},
Booktitle = {{BILDVERARBEITUNG FUR DIE MEDIZIN 2015: ALGORITHMEN - SYSTEME -
   ANWENDUNGEN}},
Year = {{2015}},
Pages = {{269-274}},
Note = {{Conference on Image Processing for Medicine (BVM), Univ Luebeck, Inst
   Med Informat, Luebeck, GERMANY, MAR 15-17, 2015}},
Organization = {{Agfa HealthCare; Olympus Surg Technologies Europe; CHILI GmbH; Haption
   GmbH; MiE GmbH; Springer Verlag GmbH; VISUS Technol Transfer GmbH}},
Abstract = {{Photographic documentation in medicine is of increasing importance.
   Efficient methods are required to properly register and calibrate the
   images. Usually, a standard reference card with special color pattern is
   placed in the aperature of the image. Localization and extraction of
   such cards is a critical step. In this paper, we adopt an iterative
   lattice detection algorithm developed for outdoor images. Once the
   lattice is extracted, crossing points of the color fields are used for
   perspective geometric transform while the color plates guide the color
   calibration process. Our method is tested on 37 images collected within
   the German Calciphylaxis Registry. At least, 28 out of the 35 possible
   grid points have been extracted in all the non-standardizes photographs,
   with at most two false positive detections. The lowest F-measure was
   above 80\%. Hence, ruler and other calibration devices become obsolete
   and wound imaging can be performed with low-cost hardware, too.}},
