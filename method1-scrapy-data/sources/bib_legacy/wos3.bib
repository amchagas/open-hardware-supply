@inproceedings{ ISI:000337300500096,
Author = {Chen, Chang-Ren and Perng, Shiang-Wuu and Yon-Chang, Hugo},
Editor = {{Meen, TH and Prior, SD and Lam, ADKT}},
Title = {{Seawater desalination using electrodialysis process}},
Booktitle = {{INNOVATION, COMMUNICATION AND ENGINEERING}},
Year = {{2014}},
Pages = {{393-395}},
Note = {{2nd International Conference on Innovation, Communication and
   Engineering (ICICE), Qingdao, PEOPLES R CHINA, OCT 26-NOV 01, 2013}},
Organization = {{China Univ Petr; Taiwanese Inst Knowledge Innovat}},
Abstract = {{This work presents a compact and low cost equipment for water
   desalination using electrodialysis (ED) with the aim to increase the
   lifetime of industrial machines that are installed near or above the sea
   and use seawater for cooling or operation purposes. The water
   desalinator equipment is mainly composed of four set of grills which are
   energized by applying a direct current (DC) to them. Two sets of them
   are positive charged and the other two sets are negative charged. The
   grills are charged in an alternative manner, it means that one set is
   negative charged followed by another set which is positive charged. The
   negative charged grills attract the positive charged ions which are
   calcium \& sodium, and the positive charged grills attract the negative
   charged ions which are chloride \& sulfate. The seawater was pretreated
   before and after the desalination process using an activated carbon
   filter to extract most of the suspended solids in the water. In addition
   different flow rates and voltages were applied to the experiment to
   observe the relationship between them and to find the parameters and
   variables that increase the efficiency of the desalination equipment.}},
