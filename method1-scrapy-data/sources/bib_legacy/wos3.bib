@inproceedings{ ISI:000379384400003,
Author = {Grammatikakis, Miltos D. and Papagrigoriou, Antonis and Petrakis,
   Polydoros and Harteros, Kostas and Kornaros, George},
Editor = {{Conti, M and Orcioni, S}},
Title = {{Load Balancing, Broadcast, and Scatter Primitives for Efficient
   Multicore Applications}},
Booktitle = {{2015 12TH INTERNATIONAL WORKSHOP ON INTELLIGENT SOLUTIONS IN EMBEDDED
   SYSTEMS (WISES)}},
