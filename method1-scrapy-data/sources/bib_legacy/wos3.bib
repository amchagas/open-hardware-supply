Title = {{Quick Deployment of Open-source Hardware and Software for a Dual-channel
   Biosignal Recorder}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON SMART INSTRUMENTATION, MEASUREMENT
   AND APPLICATIONS (ICSIMA 2013)}},
Year = {{2013}},
Note = {{IEEE International Conference on Smart Instrumentation, Measurement and
