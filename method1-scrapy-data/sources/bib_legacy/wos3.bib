@inproceedings{ ISI:000414242300149,
Author = {Deshmukh, Akshay D. and Shinde, Ulhas B.},
Book-Group-Author = {{IEEE}},
Title = {{A Low Cost Environment Monitoring System Using Raspberry Pi and Arduino
   with Zigbee}},
