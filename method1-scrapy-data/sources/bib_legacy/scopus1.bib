@ARTICLE{Bendig20151,
author={Bendig, J. and Willkomm, M. and Tilly, N. and Gnyp, M.L. and Bennertz, S. and Lenz-Wiedemann, V.I.S. and Bareth, G. and Miao, Y. and Cao, Q.},
title={Very high resolution Crop Surface Models (CSM) from UAV-based stereo images for rice growth monitoring in Northeast China},
