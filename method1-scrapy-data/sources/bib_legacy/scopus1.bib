@CONFERENCE{Pérez2013311,
author={Pérez, M. and Agüera, F. and Carvajal, F.},
title={Low cost surveying using an unmanned aerial vehicle},
journal={International Archives of the Photogrammetry, Remote Sensing and Spatial Information Sciences - ISPRS Archives},
year={2013},
