@article{ ISI:000235917900002,
Author = {Huck, T and Dostert, K and Schirmer, J},
Title = {{Low cost concept of a bit recovery FSM for incoherent DBPSK-systems in
   presence of frequency offsets}},
Journal = {{FREQUENZ}},
Year = {{2005}},
Volume = {{59}},
Number = {{11-12}},
Pages = {{246-250}},
Month = {{NOV-DEC}},
Abstract = {{This paper deals with the investigation of an innovative low-cost
   receiver design for incoherent binary DPSK-systems. Due to device
   tolerances, the generated carrier frequencies of distant clients, i.e.
   communication transceivers, have always a slight difference which is
   known as frequency offset. At present several analog methods for the
   elimination of such frequency offsets are available, which require the
   implementation of a carrier recovery circuit, i.e. in form of a
   Costas-Loop or a Squaring-Loop. Besides these coherent receiver
   concepts, incoherent systems need expensive analog-to-digital converters
   to track the rotation of the points in the constellation. The introduced
   receiver, however, uses neither a carrier recovery circuit nor expensive
   analog-to-digital converters. This new concept is realized by a finite
   state machine (FSM) forming a baseband channel selection (BCS) control,
   which will be established in this paper. Finally, simulations prove the
   high quality of such incoherent DBPSK transceivers compared to coherent
   systems, using fully digital implemented low-cost hardware for decoding.}},
