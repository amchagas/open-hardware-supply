Title = {{An integrable low cost hardware random number generator}},
Booktitle = {{Smart Structures, Devices, and Systems II, Pt 1 and 2}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
Year = {{2005}},
