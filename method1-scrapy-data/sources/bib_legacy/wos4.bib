@inproceedings{ ISI:000358305000009,
Author = {Bendig, J. and Willkomm, M. and Tilly, N. and Gnyp, M. L. and Bennertz,
   S. and Qiang, C. and Miao, Y. and Lenz-Wiedemann, V. I. S. and Bareth,
   G.},
