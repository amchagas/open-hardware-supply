Author = {Tosi, Daniele and Olivero, Massimo and Perrone, Guido},
Editor = {{Tomasini, EP}},
Title = {{Low-cost fiber Bragg grating-based static and dynamic strain system for
   structural monitoring}},
Booktitle = {{EIGHTH INTERNATIONAL CONFERENCE ON VIBRATION MEASUREMENTS BY LASER
   TECHNIQUES: ADVANCES AND APPLICATIONS}},
Series = {{Proceedings of SPIE}},
Year = {{2008}},
