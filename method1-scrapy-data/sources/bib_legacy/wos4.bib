@inproceedings{ ISI:000252739500042,
Author = {He, Xiliang and Wu, Jiehua and Gao, Xiangdong and Wu, Lingnan and Li,
   Xiaomin},
Editor = {{Yang, L and Chen, Y and Kley, YB and Li, R}},
Title = {{Comparative studies of SOR-96 SiO2 thin films prepared by spin-coating}},
Booktitle = {{ADVANCED OPTICAL MANUFACTURING TECHNOLOGIES, PTS 1 AND 2}},
Series = {{Proceedings of SPIE}},
Year = {{2007}},
Volume = {{6722}},
Number = {{1-2}},
Note = {{3rd International Symposium on Advanced Optical Manufacturing and
   Testing Technologies, Chengdu, PEOPLES R CHINA, JUL 08-12, 2007}},
Organization = {{China Opt Soc; CAS, Inst Opt \& Elect; SPIE; State Key Lab Opt Technol
   Microfabricat; Sichuan Opt Soc}},
Abstract = {{Silicon dioxide (SiO2) film is an important material for semiconductor
   industry as the gate dielectric material, and optical industry as
   antireflective coatings, and so on. The sol-gel method has been widely
   used for the deposition of oxide thin films due to several advantages
   such as simple and low-cost equipment, normal atmospheric conditions,
   and ease control of the precursor composition doping. In this work, SiO2
   thin film was deposited on BK7 glass substrate by spin-coating and
   dip-coating techniques, respectively. Three precursor concentrations
   were investigated, i.e., tetraethylorthosilicate (TEOS): ethanol: water=
   1:7:7, 1:10:10 and 1:15:15 (molar ratio). X-Ray Diffraction spectrometry
   (XRD), scanning electron microscopy (SEM), and UV-VIS-NIR scanning
   spectrophotometry were used to characterize the crystallinity,
   morphological and optical properties of deposited SiO2 thin films.
   Results show that all the SiO2 thin films deposited by the spin-coating
   and dip-coating are amorphous. SEM analysis confirms that the deposited
   SiO2 thin films have high surface quality and tight adherence with the
   substrate. The transmittance measurements indicate that dip-coated SiO2
   thin film from gel of low precursor concentration represents better
   transmittance than that from high concentrations at the range of
   600-1200nm. SiO2 thin film spin-coated from gel of high precursor
   concentration has better transmittance than that by dip-coating, but of
   opposite results for the films deposited from gels of low precursor
   concentrations.}},
Publisher = {{SPIE-INT SOC OPTICAL ENGINEERING}},
Address = {{1000 20TH ST, PO BOX 10, BELLINGHAM, WA 98227-0010 USA}},
Type = {{Proceedings Paper}},
