@inproceedings{ ISI:000286441900022,
Author = {Takei, Kentaro and Hirai, Naoyuki and Katayama, Takafumi and Song, Tian
   and Shimamoto, Takashi},
Editor = {{Qiu, GP and Lam, KM and Kiya, H and Xue, XY and Kuo, CCJ and Lew, MS}},
Title = {{Real-Time Architecture for Inter-layer Prediction of H.264/SVC}},
