Title = {{Adaptive chromatic dispersion compensation in 1.6 Tbit/s DPSK and ASK
   transmission experiments over 44...94 km of SSMF - art. no. 60212L}},
Booktitle = {{Optical Transmission, Switching, and Subsystems III, Pts 1 and 2}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
