@article{ ISI:000283954800184,
Author = {Kazakov, Artem and Furukawa, Kazuro},
Title = {{Building highly available control system applications with Advanced
   Telecom Computing Architecture and open standards}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
