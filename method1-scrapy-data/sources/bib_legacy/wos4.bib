@article{ ISI:000272006600012,
Author = {Han, Hui and MacGregor, Rodney P. and Balcom, Bruce J.},
Title = {{Pure phase encode magnetic field gradient monitor}},
Journal = {{JOURNAL OF MAGNETIC RESONANCE}},
