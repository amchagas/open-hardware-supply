Book-Group-Author = {{IEEE}},
Title = {{Real-time 3D pointing gesture recognition for natural HCI}},
Booktitle = {{2008 7TH WORLD CONGRESS ON INTELLIGENT CONTROL AND AUTOMATION, VOLS 1-23}},
Year = {{2008}},
Pages = {{2433+}},
