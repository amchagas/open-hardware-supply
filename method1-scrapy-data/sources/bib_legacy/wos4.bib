@inproceedings{ ISI:000395410100011,
Author = {Burcea, Ioana and Soares, Livio and Moshovos, Andreas},
Book-Group-Author = {{IEEE}},
Title = {{Pointy: A Hybrid Pointer Prefetcher for Managed Runtime Systems}},
Booktitle = {{PROCEEDINGS OF THE 21ST INTERNATIONAL CONFERENCE ON PARALLEL
   ARCHITECTURES AND COMPILATION TECHNIQUES (PACT'12)}},
Series = {{International Conference on Parallel Architectures and Compilation
