@inproceedings{ ISI:000406956500052,
Author = {Alves, Silas and Rosario, Joao Mauricio and Ferasoli Filho, Humberto and
   Pegoraro, Rene},
Book-Group-Author = {{IEEE Comp Soc}},
Title = {{Environment for Teaching and Development of Mobile Robot Systems}},
Booktitle = {{2010 IEEE ELECTRONICS, ROBOTICS AND AUTOMOTIVE MECHANICS CONFERENCE
   (CERMA 2010)}},
Series = {{Electronics Robotics and Automotive Mechanics Conference (CERMA)}},
Year = {{2010}},
Pages = {{302-307}},
Note = {{7th IEEE Electronics, Robotics and Automotive Mechanics Conference
   (CERMA), Cuernavaca, MEXICO, SEP 28-OCT 01, 2010}},
Organization = {{IEEE; IEEE Comp Soc; Univ Sol, Centro Nacl Investigac Desarrollo
   Tecnologico IEEE Student Branch}},
Abstract = {{Using robots for teaching is one approach that has gathered good results
   on Middle-School, High-School and Universities. Robotics gives chance to
   experiment concepts of a broad range of disciplines, principally those
   from Engineering courses and Computer Science. However, there are not
   many kits that enables the use of robotics in classroom. This article
   describes the methodologies to implement tools which serves as testbeds
   for the use of robotics to teach Computer Science and Engineering.
   Therefore, it proposes the development of a flexible, low cost hardware
   to integrate sensors and control actuators commonly found on mobile
   robots; the development of a mobile robot device whose sensors and
