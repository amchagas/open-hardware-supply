Title = {{Very high resolution crop surface models (CSMs) from UAV-based stereo
   images for rice growth monitoring In Northeast China}},
Booktitle = {{UAV-G2013}},
Series = {{International Archives of the Photogrammetry Remote Sensing and Spatial
   Information Sciences}},
