@article{ ISI:000283501400006,
Author = {Kim, Taek-Kyu and Lee, Jong-Sung and Oh, Hyeong-Cheol},
Title = {{ACCELERATING JAVA INTERPRETATION IN LOW-COST EMBEDDED PROCESSORS}},
Journal = {{JOURNAL OF CIRCUITS SYSTEMS AND COMPUTERS}},
Year = {{2010}},
Volume = {{19}},
Number = {{6}},
Pages = {{1235-1244}},
Month = {{OCT}},
Abstract = {{Hardware interpretation is an attractive choice for implementing the
   Java virtual machine (JVM) in low-end embedded systems. However, the
   hardware interpretation of complex bytecodes is so expensive that most
   low-end embedded systems rely on slow software interpreters in
   processing complex bytecodes. This paper proposes a low-cost hardware
   approach for accelerating the interpretation of eight complex bytecodes:
   four object manipulation and four method invocation bytecodes. The
   proposed approach occupies 204 LUTs in a Xilinx FPGA and reduces by up
   to 61.5\% the number of instructions executed in running the benchmarks
   considered in this paper.}},
