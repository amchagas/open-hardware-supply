["Fuel tank","Remote calibration","Internet metrology","Internet-of-Things"],"719":["User experience","prioritisation","roadmapping","smartphones","FP-tree"],"720":["engaged STS","making and doing","fracking","hydraulic fracturing","energy","oil and gas","petrochemicals","citizen science","civic science"],"721":["Open innovation","Collaborative innovation","Open innovation intermediaries","Living labs","Fab labs","Co-creation","Co-production"],"722":null,"723":["Optical stimulation","Light-emitting diodes","Embedded computers","Drosophila","Rhodopsin","Open hardware"],"724":["energy management strategy","extended range electric vehicle","dynamic programming","neural network","state of charge"],"725":null,"726":["Fab Lab","digital fabrication","open design","distributed production","sustainability"],"727":["gravity-fed surface irrigation systems","irrigation network","irrigation policy","water multi-functionality","irrigated agriculture sustainability"],"728":["bioreactor","optogenetics","two component system","adaptive laboratory evolution"],"729":null,"730":["neuro-implant","ECoG","wireless implant","open hardware","neuro-prosthetic"],"731":null,"732":["Tilt-rotor UAV","control allocation","flight strategy","flight test","tricopter configuration"],"733":["interferometry","white light","spectroscopy","Fourier transform","coherence"],"734":["flipped classroom","differentiated instruction","technology","inclusion"],"735":["Technology","Disabilities","Sensory substitution","Inclusion","Accessibility"],"736":["Indoor environmental quality (IEQ)","Smart sensor systems","User behavior","Built environment"],"737":["Internet of Things","Constrained Application Protocol","Constrained Application Protocol resources","run-time deployment","sensors and actuators"],"738":["energy-efficiency","mobile communication","proximity sensing","mobile platform","content hosting"],"739":["5G mobile network","wireless mesh networks","Better Approach to Mobile Ad hoc Networking (BATMAN)","ne